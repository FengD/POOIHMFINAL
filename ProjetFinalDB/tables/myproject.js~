var connection = require('./../database/connection.js');

exports.schema = connection.sequelize.define('Myproject', {
	name: {type: connection.Sequelize.STRING},
  date: {type: connection.Sequelize.STRING},
  from: {type: connection.Sequelize.STRING},
  Tel: {type: connection.Sequelize.STRING},
  address: {type: connection.Sequelize.STRING},
  target_money: {type: connection.Sequelize.INTEGER},
  current_money: {type: connection.Sequelize.INTEGER},
  description: {type: connection.Sequelize.STRING}
});
